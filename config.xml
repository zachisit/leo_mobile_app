<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Leo - Mobile App</name>
    <description>
        High level mobile application to create a Leo instance.
    </description>
    <author email="zach@zachis.it" href="https://zachis.it">
        zachis.it
    </author>
    <plugin name="cordova-plugin-media" source="npm" />
    <plugin name="cordova-plugin-nativeaudio" source="npm" />
    <!-- Enable all requests, inline styles, and eval() -->
    <content src="index.html" />
    <preference name="orientation" value="portrait" />
    <icon src="/img/app-icon/app-icon.png" />
    <access origin="*" />
</widget>
